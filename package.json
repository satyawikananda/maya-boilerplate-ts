{
  "name": "maya-boilerplate-ts",
  "version": "1.0.0",
  "description": "Boilerplate to create lib with ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest --coverage",
    "prettier": "prettier --check '**/*.{ts,js}'"
  },
  "keywords": [
    "boilerplate",
    "boilerplate-ts"
  ],
  "types": "./dist/index.d.ts",
  "author": "Satya Wikananda",
  "license": "MIT",
  "devDependencies": {
    "@satyawikananda/prettier-config": "^0.0.2",
    "@types/node": "^14.14.41",
    "c8": "^7.11.0",
    "prettier": "^2.5.1",
    "ts-node": "^9.1.1",
    "tsup": "^5.11.11",
    "typescript": "^4.2.4",
    "vitest": "^0.2.0"
  }
}
